{
  "$schema": "http://json-schema.org/schema",
  "id": "SchemanticsForModel",
  "title": "Model Options Schema",
  "type": "object",
  "properties": {
    "isNxWorkspace": {
      "type": "boolean",
      "default": false
    },
    "multipleApps": {
      "type": "boolean",
      "default": false
    },
    "appId": {
      "type": "string",
      "default": ""
    },
    "appIsLib": {
      "type": "boolean",
      "default": false
    },
    "appPath": {
      "type": "string",
      "default": ""
    },
    "appMainFile": {
      "type": "string",
      "default": "main.ts"
    },
    "appSelectorPrefix": {
      "type": "string",
      "default": ""
    },
    "skipModuleImport": {
      "type": "boolean",
      "default": false
    },

    "pagePath": {
      "type": "string",
      "default": ""
    },
    "formName": {
      "type": "string",
      "default": "my-form"
    }
  }
}
